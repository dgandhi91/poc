node {
    stage(‘Preparation’){
        echo 'Checking out code'
        checkout scm
    }
    stage('Build') {
        echo 'Nothing to Build as of now'
    }
    stage('Test') {
        echo 'No Tests as of now'
    }
    stage('Auto Tagging') {
        echo 'Running autotagging script'
        sh ('${WORKSPACE}/autotagging.sh')
    }
}