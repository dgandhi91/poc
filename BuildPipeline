node {
    stage('Build') {
        echo 'Checking out code'
        checkout scm
    }
    stage('Test') {
        echo 'No Tests as of now'
    }
    stage('Deploy') {
        echo 'Running autotagging script'
        pom = readMavenPom file: 'pom.xml'
        projectVersion = pom.version
        echo projectVersion
        sh ('${WORKSPACE}/autotagging1.sh ${projectVersion}')
    }
}