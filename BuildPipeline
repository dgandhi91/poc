node {
    stage('Build') {
        echo 'Checking out code'
        checkout scm
    }
    stage('Test') {
        echo 'No Tests as of now'
    }
    stage('Deploy') {
        echo 'Running autotagging script'
        POM_VERSION = readMavenPom().getVersion()
        echo 'POM Version ${POM_VERSION}'
        sh ('${WORKSPACE}/autotagging1.sh ${POM_VERSION}')
    }
}